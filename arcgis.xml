<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_height="match_parent">
 

   <com.esri.arcgisruntime.mapping.view.MapView
       android:id="@+id/arcid1"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_marginTop="320dp"
       />


 
        <EditText
            android:id="@+id/id11a"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:autofillHints=""
            android:fontFamily="@font/alice"
            android:hint="@string/idsql"
            android:inputType="number"
            android:labelFor="@+id/id"
            android:textColor="@color/black"
            android:textColorHint="#33691E" />


    </LinearLayout>
    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="52dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="21dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/alice"
            android:text="@string/date_lc"
            android:textColor="@color/black" />

        <EditText
            android:id="@+id/date11a"
            android:layout_width="66dp"
            android:layout_height="match_parent"
            android:autofillHints=""
            android:fontFamily="@font/alice"
            android:hint="@string/dd"
            android:inputType="number"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#33691E" />

        <TextView
            android:layout_width="26dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/alice"
            android:text="@string/month"
            android:textColor="@color/black" />

        <EditText
            android:id="@+id/month8a"
            android:layout_width="76dp"
            android:layout_height="60dp"
            android:autofillHints=""
            android:fontFamily="@font/alice"
            android:hint="@string/mm"
            android:inputType="number"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#33691E"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/alice"
            android:text="@string/year"
            android:textColor="@color/black" />

        <EditText
            android:id="@+id/year8a"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:autofillHints=""
            android:hint="@string/_20xx"
            android:inputType="number"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textColorHint="#33691E"
            tools:ignore="DuplicateSpeakableTextCheck" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="116dp"
        android:orientation="horizontal">


    </LinearLayout>


</RelativeLayout>
